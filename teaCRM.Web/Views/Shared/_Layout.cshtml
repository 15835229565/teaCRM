@{
    var rootPath = "";
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <!--所有页面seo开始-->
        @RenderSection("seo")
        <!--所有页面seo结束-->
        <!--母版页面js,css开始-->
        <link href="@rootPath/Themes/default/css/globle.css" rel="stylesheet" type="text/css" />
        <link href="@rootPath/Themes/default/css/head.css" rel="stylesheet" type="text/css" />
        <link href="@rootPath/Themes/default/css/foot.css" rel="stylesheet" type="text/css" />
     <script src="@rootPath/Themes/default/min/jquery.js" type="text/javascript"></script>
        <script src="@rootPath/Themes/default/js/layout.js" type="text/javascript"></script>
        <!--母版页面js,css结束-->
        <!--子页面头部js,css开始-->
        @RenderSection("head")
        <!--子页面头部js,css结束-->
    </head>
    <body>
        <!--公共头部开始-->
        <div id="header">
            <div class="header">
                <h1 class="logo"></h1>	
                <ul class="nav">
                    <li id="nav-desktop" class="selected"><a href="/Index/Index">工作台</a></li>
                    <li id="nav-customer"><a href="/Show/Index/1/">客户</a></li>
                    <li id="nav-sale"><a href="/Show/Develop/2/">销售</a></li>
                    <li id="nav-product"><a href="/Show/Develop/3/">产品</a></li>
                    <li id="nav-service"><a href="/Show/Develop/4/">服务</a></li>
                    <li id="nav-app"><a href="/Show/Develop/5/">应用</a></li>
                    <li id="nav-settings"><a href="/Settings/Department">设置</a></li>
                    <li id="nav-more"><a href="/Show/Develop/0/">更多</a></li>
                </ul>
                <div class="nav-right">
                    @*<div class="icon-option"><a href="/Settings/Department">设置</a></div>*@
                    <div class="icon-option">
                        <img alt="" src="/Themes/default/images/header.jpg" />
                        <i></i>
                    </div>
                </div>
            </div>
            
            <div id="menu">
                <!--工作台菜单-->
                <div id="menu-desktop" class="nav-header2">
                    <ul class="nav2">
                        <li class="selected"><a href="" style="color: #fff;">我的工作台</a></li>
                    </ul>
                </div>

                <!--客户模块菜单-->
                <div id="menu-customer" class="nav-header2 hide">
                    <ul class="nav2">
                        <li class="selected"><a href="" style="color: #fff;">全部客户</a></li>
                        <li><a href="">回收站客户</a></li>
                        <li><a href="">公海客户</a></li>
                        <li><a href="">我的人脉</a></li>
                    </ul>
                </div>
            
                <!--设置菜单-->
                <div id="menu-settings" class="nav-header2 hide">
                    <ul class="nav2">
                        <li class="selected"><a href="/Settings/Department" style="color: #fff;">组织架构</a></li>
                        <li><a href="/Settings/Permission">角色管理</a></li>
                        <li><a href="/Settings/Users">用户管理</a></li>
                        <li><a href="/Settings/DatabaseBackup">数据库备份</a></li>
                        <li><a href="/Settings/ExpandoField">自定义字段</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <!--公共头部结束-->
        
        <!--插件区域开始-->
        @RenderBody()
        <!--插件区域结束-->
        


        <!--公共底部开始-->
        <div id="footer">
            <div class="copyright"> copyright © 2014-2024 teaCRM.net All Rights Reserved.</div>
        </div>
        <!--公共底部结束-->
    </body>
</html>