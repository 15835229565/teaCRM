@using System.Web.Optimization
@using teaCRM.Common
@model dynamic

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section seo {
    <title>组织架构_系统设置_C+企业云平台_打造中国办公软件第一品牌</title>
    <meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
}
@section head {
    
    @Styles.Render("~/Themes/default/css/cp.globle.min8.css")
    @Scripts.Render("~/Themes/default/js/cp.globle.min8.js")

    <style type="text/css">
        .layout-left {
            display: inline;
            float: left;
            width: 190px;
            position: relative;
            z-index: 102;
            *zoom: 1;
            _height: 705px;
            background-color: #ebf0f7;
        }

        .layout-right {
            display: inline;
            float: right;
            width: 100%;
            margin-left: -190px;
            position: relative;
            z-index: 101;
            *zoom: 1;
            _height: 705px;
        }

        .layout-content {
            margin: 10px 10px 0 190px;
            position: relative;
            z-index: 102;
            *zoom: 1;
        }



        /**左侧**/

        .nav-side .ns-item {
            background: #f3f8fc;
            border-right: 1px solid #a4b8d9;
            *zoom: 1;
            min-height: 900px;
            padding: 0px 0px 10px 0px;
        }

        .ns-item .c_left {
            width: 100%;
            position: relative;
            zoom: 1;
            background: url(/Themes/default/CRM/images/c_left_btn.png) no-repeat;
            margin-bottom: 10px;
        }

        .ns-item .c_left ul {
            position: relative;
            height: 38px;
            z-index: 1;
        }

        .ns-item .c_left ul li {
            width: 92px;
            height: 38px;
            font-size: 15px;
            font-weight: 700;
            line-height: 36px;
            z-index: 1;
            cursor: pointer;
            display: inline-block;
        }

        .ns-item .c_left ul li .oz0 {
            margin-left: 30px;
            text-shadow: white 0 1px 0;
            color: #3C6692;
        }

        .ns-item .c_left ul li a:hover { color: #000; }
    </style>

    <script type="text/javascript">

    
    </script>
}

<div>
    <!--左侧开始-->
    <div class="layout-left">
        <div id="nav-side" class="nav-side">
            <div class="ns-item">
                <div class="list-group">
                    <a href="javascript:;" class="list-group-item active">
                        客户
                    </a>
                    <a href="#" class="list-group-item">客户模块</a>
                    <a href="#" class="list-group-item">联系人模块</a>
                </div>
            </div>
        </div>
    </div>
    <!--左侧结束-->
    <!-- 右侧开始 -->
    <div class="layout-right">
        <div class="layout-content">
            
               <div class="tab-content">

            <ul class="nav nav-tabs" role="tablist">
                <li class="active"><a href="#field-info" role="tab" data-toggle="tab">字段</a></li>
                <li><a href="#messages" role="tab" data-toggle="tab">视图</a></li>
                <li><a href="#settings" role="tab" data-toggle="tab">操作</a></li>
            </ul>

         
                <div class="tab-pane active" id="field-info">
                    <table id="field-grid-data" class="table table-condensed table-hover table-striped table-bordered" data-selection="true" data-multi-select="true">
                        <thead>
                            <tr>
                                <th data-column-id="Id" data-type="numeric" data-identifier="true">字段ID</th>
                            </tr>
                        </thead>
                    </table>
                </div>
                <div class="tab-pane" id="messages">
                    <table id="view-grid-data" class="table table-condensed table-hover table-striped table-bordered" data-selection="true" data-multi-select="true">
                        <thead>
                            <tr>
                                <th data-column-id="Id" data-type="numeric" data-identifier="true">视图ID</th>
                            </tr>
                        </thead>
                    </table>
                </div>
                <div class="tab-pane" id="settings">
                    <table id="toolbar-grid-data" class="table table-condensed table-hover table-striped table-bordered" data-selection="true" data-multi-select="true">
                        <thead>
                            <tr>
                                <th data-column-id="Id" data-type="numeric" data-identifier="true">操作ID</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <!-- 右侧结束 -->
</div>
@section script{
    
}